{
  "compilerOptions": {                                   
    "lib": ["dom", "dom.Iterable","esnext"],                                       
    "moduleResolution": "node",                       
    "resolveJsonModule": true,                        
    "allowJs": true,                                  
    "isolatedModules": true,                          
    "allowSyntheticDefaultImports": true,             
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true,
    "jsx":"react-jsx",
    "noEmit": true,                                 
  },
  "include": ["src"]

}
